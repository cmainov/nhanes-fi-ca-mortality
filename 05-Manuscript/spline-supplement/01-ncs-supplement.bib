	author = {De Vito, R. and Lee, Yuan Chin Amy and Parpinel, M. and Serraino, D. and Olshan, Andrew Fergus and Zevallos, Jose Pedro and Levi, F. and Zhang, Zhuo Feng and Morgenstern, H. and Garavello, W. and Kelsey, K. and McClean, M. and Schantz, S. and Yu, Guo Pei and Boffetta, P. and Chuang, Shu Chun and Hashibe, M. and La Vecchia, C. and Parmigiani, G. and Edefonti, V.},
