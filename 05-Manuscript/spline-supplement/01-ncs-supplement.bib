	pmid = {11382797},
	keywords = {Humans, Aged, Female, Male, Nutrition Assessment, Nutrition Disorders, Mass Screening, Discriminant Analysis, Geriatrics, Professional Practice},
	pages = {M366--372},
	file = {Full Text:/Users/Chris/Zotero/storage/BY2UQLHK/Rubenstein et al. - 2001 - Screening for undernutrition in geriatric practice.pdf:application/pdf},
