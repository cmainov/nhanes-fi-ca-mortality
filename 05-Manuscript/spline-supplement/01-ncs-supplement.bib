	keywords = {Humans, Neoplasms, Phytochemicals, Cardiovascular Diseases, Micronutrients, Diabetes Mellitus, Clinical Trials as Topic, Plant Extracts, Catechin, Curcuma, Curcumin, Dementia, Resveratrol, Stilbenes, Tea, Vitis, Wine},
	pages = {558--566},
}

