	pmid = {24069513},
	pmcid = {PMC3781223},
	keywords = {Acute toxicity, Chemoradiotherapy, Dysphagia, Head and neck cancer, Late toxicity, Malnutrition, Weight loss},
	pages = {117--126},
	file = {Full Text:/Users/Chris/Zotero/storage/PJK67472/Denaro et al. - 2013 - Dysphagia in Head and Neck Cancer Patients Pretre.pdf:application/pdf},
