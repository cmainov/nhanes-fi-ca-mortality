	pmid = {24440543},
	keywords = {Humans, Middle Aged, Female, Risk Factors, Male, Chronic Disease, Cross-Sectional Studies, Health Services Needs and Demand, Food Supply, Costs and Cost Analysis, Socioeconomic Factors, Chronic disease, Medication adherence, Medication Adherence, Population health, Socioeconomic factors, Underserved populations},
	pages = {303--310.e3},
	file = {Submitted Version:/Users/Chris/Zotero/storage/4TIHJWM4/Berkowitz et al. - 2014 - Treat or eat food insecurity, cost-related medica.pdf:application/pdf},
}

