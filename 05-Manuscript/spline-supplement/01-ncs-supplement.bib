	author = {Deneo-Pellegrini, Hugo and Boffetta, Paolo and De Stefani, Eduardo and Correa, Pelayo and Ronco, Alvaro L. and Acosta, Gisele and Mendilaharsu, María and Silva, Cecilia and Luaces, María E.},
