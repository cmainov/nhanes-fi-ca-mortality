	keywords = {Humans, Adult, Aged, Head and Neck Neoplasms, Middle Aged, Female, Male, Aged, 80 and over, Diet, Adolescent, Interpersonal Relations, Quality of Life, Combined Modality Therapy, Body Image, Emotions, Feeding and Eating Disorders, Severity of Illness Index, Speech Disorders},
