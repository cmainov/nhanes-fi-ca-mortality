	keywords = {Humans, Adult, Body Mass Index, Body Weight, Middle Aged, Female, Risk Factors, Prospective Studies, Obesity, Diabetes Mellitus, Type 2, Polycystic Ovary Syndrome, Overweight, Polycystic ovary syndrome, BMI, Glucose Tolerance Test, prediabetes, Prediabetic State, Type 2 diabetes mellitus, weight gain},
