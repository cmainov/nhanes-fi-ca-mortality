	year = {2018},
	pmid = {29896935},
	keywords = {Humans, Female, systematic review, Randomized Controlled Trials as Topic, Polycystic Ovary Syndrome, Exercise Therapy, Exercise training, Heart, Menstrual Cycle, polycystic ovary syndrome, Pregnancy, reproduction, Reproduction},
	pages = {275--284},
}

