@article{mcnabney_short_2017,
	title = {Short {Chain} {Fatty} {Acids} in the {Colon} and {Peripheral} {Tissues}: {A} {Focus} on {Butyrate}, {Colon} {Cancer}, {Obesity} and {Insulin} {Resistance}},
	volume = {9},
