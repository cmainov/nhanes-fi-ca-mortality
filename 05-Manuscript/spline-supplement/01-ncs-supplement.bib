	keywords = {Humans, Head and Neck Neoplasms, Middle Aged, Female, Risk Factors, Male, Multivariate Analysis, Proportional Hazards Models, Prospective Studies, Cohort Studies, United States, Follow-Up Studies, Dietary Fiber, Confidence Intervals, National Institutes of Health (U.S.), Sex Factors, Edible Grain},
	pages = {1405--1414},
