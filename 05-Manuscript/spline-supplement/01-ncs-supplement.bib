	keywords = {Humans, Colorectal Neoplasms, Middle Aged, Risk Factors, Male, Insulin-Like Growth Factor I, Risk, Case-Control Studies, Insulin-Like Growth Factor Binding Protein 3, Prospective Studies, Insulin-Like Growth Factor II},
	pages = {620--625},
