	journal = {Nutrients},
	author = {Bascuñán, Karla A. and Elli, Luca and Pellegrini, Nicoletta and Scricciolo, Alice and Lombardo, Vincenza and Doneda, Luisa and Vecchi, Maurizio and Scarpa, Cecilia and Araya, Magdalena and Roncoroni, Leda},
