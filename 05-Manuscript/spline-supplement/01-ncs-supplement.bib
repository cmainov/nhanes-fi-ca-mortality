	pmid = {1632423},
	keywords = {Humans, Male, Prospective Studies, Feeding Behavior, Surveys and Questionnaires, Health Personnel, Diet Surveys, Age Factors, Reproducibility of Results, Nutrition Surveys, Boston, Diet Records},
	pages = {1114--1126; discussion 1127--1136},
}

