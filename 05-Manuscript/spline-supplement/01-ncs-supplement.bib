	keywords = {Humans, Adult, Aged, Body Composition, Middle Aged, Biomarkers, Female, Risk Assessment, Risk Factors, Male, Logistic Models, Odds Ratio, Prospective Studies, Cohort Studies, Dietary Fiber, Confidence Intervals, Exercise, Cross-Sectional Studies, Cardiovascular Diseases, C-Reactive Protein, Linear Models, Longitudinal Studies, Solubility},
