@article{gibbs_assessing_2016,
	title = {Assessing the {Nutrition} {Literacy} of {Parents} and {Its} {Relationship} {With} {Child} {Diet} {Quality}},
	volume = {48},
	issn = {1878-2620},
	doi = {10.1016/j.jneb.2016.04.006},
	abstract = {OBJECTIVE: To estimate the reliability and validity of the Nutrition Literacy Assessment Instrument for Parents (NLit-P) and to investigate relationships among parental nutrition literacy, parental and child body mass index, and child diet quality (Healthy Eating Index).
METHODS: Cross-sectional study of 101 parent-child dyads that collected measures of socioeconomic status, nutrition literacy, 2 24-hour child diet recalls, and body mass index. Reliability of NLit-P was assessed by confirmatory factor analysis. Pearson correlation and multiple linear regression were used.
RESULTS: Fair to substantial reliability was seen across 5 NLit-P domains, whereas Pearson correlations support concurrent validity for the NLit-P related to child diet quality and parental income, age, and educational attainment (P {\textless} .001). For every 1\% increase in NLit-P, there was a 0.51 increase in child Healthy Eating Index (multivariate coefficient, 0.174; P {\textless} .001).
CONCLUSIONS AND IMPLICATIONS: The NLit-P demonstrates potential for measuring parental nutrition literacy, which may be an important educational target for improving child diet quality.},
