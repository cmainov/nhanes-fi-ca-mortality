	number = {2},
	journal = {JCO oncology practice},
	author = {Panzone, John and Welch, Christopher and Morgans, Alicia and Bhanvadia, Sumeet K. and Mossanen, Matthew and Goldberg, Rachel Shenhav and Chandrasekar, Thenappan and Pinkhasov, Ruben and Shapiro, Oleg and Jacob, Joseph M. and Basnet, Alina and Bratslavsky, Gennady and Goldberg, Hanan},
