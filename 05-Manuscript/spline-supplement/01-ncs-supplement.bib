	keywords = {Humans, Energy Metabolism, Risk Factors, Dietary Carbohydrates, Dietary Fats, Cardiovascular Diseases, Energy Intake, Nutrition Policy, Diet, Healthy, Nutritional Sciences, Consumer Behavior, Dietary Fats, Unsaturated},
