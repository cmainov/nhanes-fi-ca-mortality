	keywords = {diet, dietary pattern, schizophrenia},
	pages = {1115--1120},
	file = {Full Text:/Users/Chris/Zotero/storage/RXUY5CBA/Tsuruga et al. - 2015 - Dietary patterns and schizophrenia a comparison w.pdf:application/pdf},
}

