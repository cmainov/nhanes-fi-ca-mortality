	month = oct,
	year = {2015},
	pmid = {26060053},
	pmcid = {PMC4698101},
	keywords = {lifestyle behaviors, oncology, psychosocial, survivors, symptoms},
	pages = {1250--1257},
	file = {Accepted Version:/Users/Chris/Zotero/storage/NSP3XH48/Bluethmann et al. - 2015 - Grasping the 'teachable moment' time since diagno.pdf:application/pdf},
}

