	urldate = {2023-03-14},
	journal = {Journal of the American College of Nutrition},
	author = {Guillermo, Cherie and Boushey, Carol J. and Franke, Adrian A. and Monroe, Kristine R. and Lim, Unhee and Wilkens, Lynne R. and Marchand, Loïc Le and Maskarinec, Gertraud},
