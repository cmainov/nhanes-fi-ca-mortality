	keywords = {Humans, Adult, Aged, Breast Neoplasms, Middle Aged, Female, Diet, Surveys and Questionnaires, Pilot Projects, Quality of Life, Nutrition Assessment, Nutritional Status, Health Knowledge, Attitudes, Practice, Health Literacy, Cancer Survivors, Patient Education as Topic, Health literacy, Health education, Nutrition therapy education},
	pages = {493--499},
