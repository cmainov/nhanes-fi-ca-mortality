	keywords = {Humans, Adult, Treatment Outcome, Female, Risk Factors, Male, Logistic Models, Proportional Hazards Models, Time Factors, Prospective Studies, Follow-Up Studies, Population Surveillance, Cardiovascular Diseases, Confounding Factors, Epidemiologic, Propensity Score, Socioeconomic Factors, Drug Monitoring, Hydroxymethylglutaryl-CoA Reductase Inhibitors, Primary Prevention, Time},
