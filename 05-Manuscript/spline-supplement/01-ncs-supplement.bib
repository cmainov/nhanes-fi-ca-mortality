	pmcid = {PMC2717751},
	pages = {3437--3444},
	file = {PubMed Central Full Text PDF:/Users/Chris/Zotero/storage/S2GXXE27/Pierce et al. - 2009 - Elevated Biomarkers of Inflammation Are Associated.pdf:application/pdf},
}

