	journal = {Journal of Clinical Oncology},
	author = {Pierce, Brandon L. and Ballard-Barbash, Rachel and Bernstein, Leslie and Baumgartner, Richard N. and Neuhouser, Marian L. and Wener, Mark H. and Baumgartner, Kathy B. and Gilliland, Frank D. and Sorensen, Bess E. and McTiernan, Anne and Ulrich, Cornelia M.},
	month = jul,
