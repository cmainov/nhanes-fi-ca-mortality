	pmid = {18413553},
	keywords = {Humans, Adult, Middle Aged, Female, Risk, Prospective Studies, Cohort Studies, Diet, Coronary Disease, Cross-Sectional Studies, Hypertension, C-Reactive Protein, Interleukin-6, Myocardial Infarction, Stroke},
	pages = {713--720},
