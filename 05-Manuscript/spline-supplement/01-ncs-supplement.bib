	keywords = {Humans, Adult, Middle Aged, Female, Ovarian Neoplasms, Cohort Studies, Glycemic Index, Dietary Carbohydrates, Incidence, Surveys and Questionnaires, Canada, Diet Surveys},
	pages = {1076--1081},
