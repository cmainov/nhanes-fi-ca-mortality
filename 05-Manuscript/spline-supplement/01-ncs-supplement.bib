	pmcid = {PMC9028465},
	keywords = {Humans, Aged, Female, Male, Mental Disorders, ageism, Ageism, COVID-19, Pandemics, SARS-CoV-2, COVID-19 age discrimination, COVID-19 outbreak, COVID-19 risk perception, fear of COVID-19, loneliness, Loneliness, mental illness, social isolation, Social Isolation, Social Perception},
	pages = {4513},
